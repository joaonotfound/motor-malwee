{
  "name": "motor-vendas",
  "version": "1.0.0",
  "main": "./dist/main/index.js",
  "author": {
    "name": "@joaonotfound"
  },
  "license": "MIT",
  "dependencies": {
    "@mikro-orm/core": "^5.4.2",
    "@mikro-orm/mysql": "^5.4.2",
    "express": "^4.18.2",
    "jsonwebtoken": "^8.5.1",
    "mikro-orm": "^5.4.2",
    "tslint": "^6.1.3",
    "typescript": "^4.8.4"
  },
  "files": [
    "./bin/*",
    "./lib/*"
  ],
  "typings": "./lib/index.d.ts",
  "scripts": {
    "start": "rm -rf ./dist && npm run build && tsc-alias && node ./dist/main/index.js",
    "build": "tsc",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "prepublish": "npm run build"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  },
  "devDependencies": {
    "@mikro-orm/cli": "^5.4.2",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "tsc-alias": "^1.7.0"
  }
}
